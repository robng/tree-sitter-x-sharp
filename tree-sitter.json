  "grammars": [
    {
      "name": "xsharp",
      "camelcase": "XSharp",
      "title": "X#",
      "scope": "source.xsharp",
      "file-types": [
        "xsharp"
      ],
      "injection-regex": "^xsharp$",
      "class-name": "TreeSitterXsharp"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the X# language",
    "authors": [
      {
        "name": "robng",
        "email": "49914302+robng@users.noreply.github.com",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/robng/tree-sitter-x-sharp",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "java": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
